   <children>
      <VBox alignment="CENTER" layoutX="226.0" layoutY="9.0" prefHeight="360.0" prefWidth="149.0" spacing="20.0" AnchorPane.bottomAnchor="15.0" AnchorPane.topAnchor="15.0">
         <children>
            <Label text="Login" />
            <TextField fx:id="textFieldUsername" promptText="Username" />
            <TextField fx:id="textFieldPassword" promptText="Password" />
            <Button fx:id="btnLogin" defaultButton="true" mnemonicParsing="false" onAction="#onButtonLoginClick" text="Login" />
         </children>
      </VBox>
   </children>
</AnchorPane>
